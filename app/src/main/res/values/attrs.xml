<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="DefaultTextView">
        <attr name="defaultBackgroundResource" format="reference"/>
    </declare-styleable>



    <declare-styleable name="MyView0">
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>



    <declare-styleable name="MyView01">
        <attr name="firstColor"  format="color"/>
        <attr name="secondColor" format="color"/>
        <attr name="circleWidth" format="dimension"/>
        <attr name="speed"  format="integer"/>
    </declare-styleable>


    <declare-styleable name="ContentText">
        <attr name="text_empty_color" format="color"/>
        <attr name="text_no_empty_color" format="color"/>
    </declare-styleable>


    <declare-styleable name="DefaultBGTextView">
        <attr name="defaultBackground" format="reference"/>
        <attr name="defaultMinWidth" format="integer"/>
        <attr name="backgroundResource" format="reference"/>
    </declare-styleable>



    <declare-styleable name="RoundImageView">
        <attr name="roundRadius" format="integer"/>
        <attr name="circleRadius" format="integer"/>
        <attr name="type" format="enum">
            <enum name="circle" value="1"/>
            <enum  name="round" value="2" />
        </attr>
    </declare-styleable>
    
    <declare-styleable name="GuaGuaLeView">
        <attr name="paintWidth" format="integer"/>
        <attr name="guaGuaBgColor" format="color"/>
        <attr name="bgId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ShadowLayerView">
        <attr name="elevate" format="integer"/>
        <attr name="elevationColor" format="color"/>
        <attr name="shape" format="enum">
            <enum name="shapeRect" value="1"/>
            <enum name="circle" value="2"/>
            <enum name="round" value="3"/>
        </attr>

        <attr name="roundRx" format="integer"/>
        <attr name="roundRy" format="integer"/>
    </declare-styleable>
</resources>